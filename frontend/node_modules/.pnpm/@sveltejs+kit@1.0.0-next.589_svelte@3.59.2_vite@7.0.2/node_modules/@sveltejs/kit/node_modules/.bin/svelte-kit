#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules/@sveltejs/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules/@sveltejs/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@7.0.2/node_modules:/mnt/c/Users/acer/issues-insights-tracker/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
